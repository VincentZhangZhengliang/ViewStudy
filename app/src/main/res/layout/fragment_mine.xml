<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:vincent="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.topzrt.viewstudy.MineFragment">

    <com.topzrt.viewstudy.MagicCircle
        android:id="@+id/id_mc"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <Button
        android:id="@+id/id_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:background="#55caca"
        android:text="go"
        android:textColor="#fff" />


    <com.topzrt.viewstudy.ProgressView
        android:id="@+id/combineview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        vincent:checked="false"
        vincent:text="Vincent" />


    <com.topzrt.viewstudy.VProgressView
        android:id="@+id/vprogressview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <com.topzrt.viewstudy.LockView
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</RelativeLayout>
